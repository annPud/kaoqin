<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<!-- 包扫描 -->
	<context:component-scan base-package="com.zhu.kaoqin.repository" />
	<context:component-scan base-package="com.zhu.kaoqin.service" />

	<bean id="springBeanFactory" class="com.zhu.kaoqin.utils.SpringBeanFactory" />
	<import resource="spring-core-datasource.xml" />
	<import resource="spring-core-druid.xml" />
	<import resource="spring-core-JDBC.xml" />
</beans>